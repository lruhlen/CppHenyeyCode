#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <fstream>
#include <sstream>
#include <vector>
#include <math.h>
#include <string.h>
#include <float.h>
#include "global.h"
using namespace std;

vec2d unflattenMatrix(OneD &array, int i_index, int j_index)
{
  result = vec2d(i_index,j_index,0);

  return result;
}

vec3d unflattenMatrix(OneD &array, int i_index, int j_index, int k_index)
{
  result = vec3d(i_index,j_index,k_index,0);

  return result;
}
